{
    "version": 2,
    "controller": {},
    "workers": [
        {
            "type": "router",
            "realms": [
                {
                    "name": "realm1",
                    "roles": [
                        {
                            "name": "anonymous",
                            "permissions": [
                                {
                                    "uri": "",
                                    "match": "prefix",
                                    "allow": {
                                        "call": true,
                                        "register": true,
                                        "publish": true,
                                        "subscribe": true
                                    },
                                    "disclose": {
                                        "caller": false,
                                        "publisher": false
                                    },
                                    "cache": true
                                }
                            ]
                        }
                    ]
                }
            ],
            "transports": [
                {
                    "type": "universal",
                    "endpoint": {
                        "type": "tcp",
                        "port": 55058
                    },
                    "rawsocket": {
                    },
                    "websocket": {
                        "ws": {
                            "type": "websocket"
                        }
                    },
                    "web": {
                        "paths": {
                            "/": {
                                "type": "static",
                                "directory": ".."
                            }
                        }
                    }
                }
            ]
        },
        {
            "id": "WampADCP",
            "type": "guest",
            "executable": "python3",
            "arguments": [
                "WampAdcp.py",
                "--port",
                "/dev/tty.usbserial-FTYNODPO",
                "--baud",
                "460800"
            ],
            "options": {
                "workdir": "../../Wamp",
                "env": {
                    "inherit": true,
                    "vars": {
                        "WS": "ws://localhost:8080/ws"
                    }
                }
            }
        }
    ]
}
